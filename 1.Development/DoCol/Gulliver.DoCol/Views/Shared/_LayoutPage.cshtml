<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
	<title>@ViewBag.Title</title>
	@Styles.Render( "~/bundles/style" )
	@RenderSection( "CssSection", required: false )

	@Scripts.Render( "~/bundles/jquery" )
	@Scripts.Render( "~/bundles/jqueryui" )
	@Scripts.Render( "~/bundles/jqueryval" )
	@Scripts.Render( "~/bundles/customjs" )

	<meta name="viewport" content="width=device-width" />
    <meta name="referrer" content="always"/>
</head>
<body>
	<div id="loading"></div>
	@Html.Partial( "~/Views/Shared/_TopMenuPartial.cshtml" )
	<div class="col-xs-12"></div>
	<div class="col-xs-12 title">
		@RenderSection( "TitleSection", required: false )
	</div>
	<div class="container no_pa">
		@RenderBody()

		<div class="scrollup"></div>
	</div>
    <div id="footer">
      <div class="container">
        <h6><span>@System.Configuration.ConfigurationManager.AppSettings["Version"]</span>
            <span style="float:right;"><a href="https://sites.google.com/a/cloud.glv.co.jp/manual/home/IT-System/juhacchu/juhacchu-system" target="_blank">マニュアルはこちら</a></span></h6>
    </div>
		
	</div>
	<!-- content error validate-->
	<div id="HiddenErrorValidate" class="hidden">@ViewBag.ErrorValidate</div>
	@Scripts.Render( "~/bundles/jqueryCommon" )
	@RenderSection( "Scripts", required: false )
	@RenderSection( "Styles", required: false )
</body>
</html>
